DATA_BLOCK "SelvMaLiftFrame_Hmi"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
   STRUCT 
      tLiftFrame : Array["STARTARRAY".."DNUMLIFTFRAME"] of Struct
         tHMR : Struct   // {[Unit]|[Scada Unit]|[LowLimit]|[HigLimit]|[Acces]}
            xEnabled : Bool;   // Enabled - lift Frame {x|x|x|x|O}
            xHighPos : Bool;   // Lift frame Up Position
            xDownPos : Bool;   // Lift frame Down Position
            xManualCommandEnabled : Bool;   // Manual Mode Enable {x|x|x|x|O}
            iStateTape : Int;   // State (1=No alarm, 3=Run, 7=Alarm) {x|x|x|x|O}
            iStateLiftFrame : Int;   // State (1=No alarm, 3=Run, 7=Alarm) {x|x|x|x|O}
            tManCmd : Struct
               xRise : Bool;   // Rise Command {x|x|x|x|O}
               xDescend : Bool;   // Descend Command {x|x|x|x|O}
               xRunTape : Bool;   // Run tape Command {x|x|x|x|O}
               xReverseTape : Bool;   // Reverse tape Command {x|x|x|x|O}
            END_STRUCT;
         END_STRUCT;
         tHMW : Struct   // {[Unit]|[Scada Unit]|[LowLimit]|[HigLimit]|[Acces]}
            xEnable : Bool;   // Enabled - Lift Frame  {x|x|x|x|O}
            xDisable : Bool;   // Enabled - Lift Frame  {x|x|x|x|O}
            xEnableDisableManualCommand : Bool;   // Enable/Disable manual command{x|x|x|x|O}
            tManCmd : Struct
               xRise : Bool;   // Rise Command {x|x|x|x|O}
               xDescend : Bool;   // Descend Command {x|x|x|x|O}
               xRunTape : Bool;   // Run tape Command {x|x|x|x|O}
               xReverseTape : Bool;   // Reverse tape Command {x|x|x|x|O}
            END_STRUCT;
         END_STRUCT;
      END_STRUCT;
   END_STRUCT;


BEGIN

END_DATA_BLOCK

