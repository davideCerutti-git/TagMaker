DATA_BLOCK "SelvMacBrWh_Hmi"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
   STRUCT 
      tWh : Array["STARTARRAY".."DNUMWHEEL"] of Struct
         tHMR : Struct   // {[Unit]|[Scada Unit]|[LowLimit]|[HigLimit]|[Acces]}
            xEnabled : Bool;   // Enabled - Frame {x|x|x|x|O}
            xSelctorEnableHead : Bool;   // Selector Enabled
            xSelctorDisableHead : Bool;   // Selector Disabled
            xWheelDown : Bool;   // Wheel Down Position (on work)
            xBlowChipsEnabled : Bool;   // Blow Chips Enabled
            xDynamicModeEnabled : Bool;   // Dynamic - Mode {x|x|x|x|O}
            xManualCommandEnabled : Bool;   // Manual Mode Enable {x|x|x|x|O}
            iState : Int;   // State (1=No alarm, 3=Run, 7=Alarm) {x|x|x|x|O}
            rEncoderPosition_mm : Real;   // Encoder Position  {mm|mm|x|x|O}
            dEncoderPosition_cmm : DInt;   // Encoder Position {cmm|cmm|x|x|O}
            rActualPosition_mm : Real;   // Actual Position {mm|mm|x|x|O}
            dActualPosition_cmm : DInt;   // Actual Position {cmm|cmm|x|x|O}
            tManCmd : Struct
               xDescend : Bool;   // Descend Command {x|x|x|x|O}
               xBlowChips : Bool;   // Blow Chips Command {x|x|x|x|O}
            END_STRUCT;
         END_STRUCT;
         tHMW : Struct   // {[Unit]|[Scada Unit]|[LowLimit]|[HigLimit]|[Acces]}
            xEnable : Bool;   // Enabled - Frame  {x|x|x|x|O}
            xDisable : Bool;   // Enabled - Frame  {x|x|x|x|O}
            xDynamicModeSeparation : Bool;   // Dynamic - Mode Separation {x|x|x|x|O}
            xStaticModeSeparation : Bool;   // Static  - Mode Separation {x|x|x|x|O}
            xEnableDisableBlowChips : Bool;   // Enable/Disable BlowChips {x|x|x|x|O}
            xPresetEncoder : Bool;   // Preset Encoder  {x|x|x|x|O}
            iPlateLenghtRiseRoll_perc : Int;   // Percent of Break Down Durin the Dinamic  {x|x|x|x|O}
            dReactionTime_ms : DInt;   // Reaction time {ms|ms|x|x|O}
            dOffsetRise_cmm : DInt;   // Offset rise bar {mm/100|mm|x|x|O}
            dOffsetDescent_cmm : DInt;   // Offset descent bar {mm/100|mm|x|x|O}
            dReactionTime_s : Real;   // Reaction time {s|s|x|x|O}
            rOffsetRise_mm : Real;   // Offset rise bar [{mm|mm|x|x|O}
            rOffsetDescent_mm : Real;   // Offset descent bar {mm|mm|x|x|O}
            xEnableDisableManualCommand : Bool;   // Enable/Disable manual command{x|x|x|x|O}
            tManCmd : Struct
               xDescend : Bool;   // Descend Command {x|x|x|x|O}
               xBlowChips : Bool;   // Blow Chips Command {x|x|x|x|O}
            END_STRUCT;
         END_STRUCT;
      END_STRUCT;
   END_STRUCT;


BEGIN

END_DATA_BLOCK

DATA_BLOCK "SelvMacBrWh_Alm"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
NON_RETAIN
   STRUCT 
      tWh : Array["STARTARRAY".."DNUMWHEEL"] of Struct
         tEmergencyAlarm : Array["STARTARRAY".."DALARM"] of "UDT_HMI_Alarm_V2";   // stop machine fault
         tAlarm : Array["STARTARRAY".."DALARM"] of "UDT_HMI_Alarm_V2";   // stop next cycle faults
         tWarning : Array["STARTARRAY".."DALARM"] of "UDT_HMI_Alarm_V2";   // Generic warning
      END_STRUCT;
   END_STRUCT;


BEGIN

END_DATA_BLOCK

