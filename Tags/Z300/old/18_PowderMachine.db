DATA_BLOCK "Powder_Alm"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
NON_RETAIN
   STRUCT 
      tEmergencyAlarm { S7_SetPoint := 'False'} : Array["STARTARRAY".."DALARM"] of "UDT_HMI_Alarm_V2";   // stop machine fault
      tAlarm { S7_SetPoint := 'False'} : Array["STARTARRAY".."DALARM"] of "UDT_HMI_Alarm_V2";   // stop next cycle faults
      tWarning { S7_SetPoint := 'False'} : Array["STARTARRAY".."DALARM"] of "UDT_HMI_Alarm_V2";   // Generic warning
   END_STRUCT;


BEGIN

END_DATA_BLOCK

DATA_BLOCK "Powder_Hmi"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
   STRUCT 
      tHMR : Struct   // {[Unit]|[Scada Unit]|[LowLimit]|[HigLimit]|[Acces]}
         xAutoinclusion : Bool;   // Automatic powder inclusion {x|x|x|x|O}
         xPowederEnaled : Bool;
         iState : Int;   // State (1=No anomaly, 3=Run, 7=Anomaly) {x|x|x|x|O}
         rgxNozzleActivate : Array["STARTARRAY".."NUMOFNOZZLES"] of Bool;   // Nozzle active {x|x|x|x|O}
      END_STRUCT;
      tHMW : Struct   // {[Unit]|[Scada Unit]|[LowLimit]|[HigLimit]|[Acces]}
         rOffsetStart_mm : Real;   // Offset start powder [mm] {mm|mm|-300|300|O}
         rOffsetStop_mm : Real;   // Offset stop powder [mm] {mm|mm|-300|300|O}
         dOffsetStart_cmm : DInt;   // Offset start powder [mm/100] {mm/100|mm|-300|300|O}
         dOffsetStop_cmm : DInt;   // Offset stop powder [mm/100] {mm/100|mm|-300|300|O}
         dReactionTime_ms : DInt;   // Reaction time [ms] {ms|s|0|4|O}
         dPowder_Perc : DInt;   // Powder Perc
         xEnableDisablePowder : Bool;
         xReqAutoInclusion : Bool;   // Request automatic powder inclusion {x|x|x|x|O}
         xReqManuaInclusion : Bool;   // Request manual powder inclusion  {x|x|x|x|O}
         rgxNozzleEnableDisableActivate : Array["STARTARRAY".."NUMOFNOZZLES"] of Bool;   // Nozzle active {x|x|x|x|O}
      END_STRUCT;
   END_STRUCT;


BEGIN

END_DATA_BLOCK

